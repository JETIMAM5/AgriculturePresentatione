@using EntityLayer.Concrete
@model List<Address>

<h3 class="tittle">Bizimle İletişime Geçin</h3>
<p class="heading-btm-w3ls">Aşağıdaki form üzerinden bize mail göderebilir veya telefon numarası üzerinden bizimle iletişime geçebilrisiniz.</p>
@foreach (var item in Model)
{
    <div class="col-md-3 contact-left">
        <h4>Bizim Adresimiz</h4>
        <p>@item.Description1</p>
        <p>@item.Description2</p>
        <p>@item.Description3</p>
        <h4>Phone</h4>
        <p>@item.Description4</p>

    </div>
}